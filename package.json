{
  "name": "contract-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./src/bin/www",
    "lint": "eslint --ignore-path .gitignore .",
    "migrate": "node ./bin/sync.js",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && node ./src/scripts/syncDB.js ",
    "make-model": "sequelize migration:generate --name",
    "down": "npx sequelize-cli db:migrate:undo",
    "make-seed": "npx sequelize-cli seed:generate --name",
    "seed": "node ./seeders/Admin.js",
    "undo-migrate": "npx sequelize-cli db:migrate:undo",
    "seed-specific": "npx sequelize-cli db:seed --seed",
    "db:sync": "node ./bin/sync.js",
    "sync:db": "node src/scripts/syncDB.js",
    "seed:db": "node src/scripts/seedDB.js",
    "seedRole:db": "node src/scripts/seedRole.js"
  },
  "dependencies": {
